class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int> map;
        int n=nums.size();
        for(int i=0;i<n;i++){
            map[nums[i]]++;
        }
        unordered_map<int,int>::iterator it;
        for(it=map.begin();it!=map.end();it++){
            if(it->second>(n/2))
                break;
        }
        return it->first;
    }
};
