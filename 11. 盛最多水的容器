class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size();
        int i=0;
        int j=n-1;
        int m=0;
        int count=0;
        while(i<j){
            if(height[i]<=height[j]){
                m=height[i]*(j-i);
                i++;
            }else{
                m=height[j]*(j-i);
                j--;
            }
            count=(count>m ? count:m);
        }
        return count;
    }
};
